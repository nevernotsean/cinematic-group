{% if field %}

  {% if field == 'spotify' %}

  <i class="fa fa-spotify" aria-label="Spotify"></i>

   {% elseif field == 'itunes' %}

  <i class="fa fa-music" aria-label="iTunes"></i>

    {% elseif field == 'apple' %}

  <i class="fa fa-apple" aria-label="Apple Music"></i>

    {% elseif field == 'soundcloud' %}

  <i class="fa fa-soundcloud" aria-label="Soundcloud"></i>

    {% elseif field == 'youtube' %}

  <i class="fa fa-youtube-play" aria-label="Youtube"></i>

    {% elseif field == 'Amazon' %}

  <i class="fa fa-amazon" aria-label="Amazon MP3"></i>

    {% elseif field == 'Google Play' %}

  <i class="fa fa-play" aria-label="Google Play"></i>

    {% elseif field == 'facebook' %}

  <i class="fa fa-facebook" aria-label="Facebook"></i>

    {% elseif field == 'twitter' %}

  <i class="fa fa-twitter" aria-label="Twitter"></i>

    {% elseif field == 'website' %}

  <i class="fa fa-globe" aria-label="Website"></i>

    {% elseif field == 'tumblr' %}

  <i class="fa fa-tumblr" aria-label="Tumblr"></i>

  {% elseif field == 'instagram' %}

  <i class="fa fa-instagram" aria-label="Instagram"></i>

{% elseif field == 'snapchat' %}

  <i class="fa fa-snapchat" aria-label="Snapchat"></i>

    {% else %}

  {% endif %}

{% endif %}
